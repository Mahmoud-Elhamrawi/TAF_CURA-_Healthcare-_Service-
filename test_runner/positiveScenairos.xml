<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="positive TC" verbose="1">
    <listeners>
        <listener class-name="Listeners.IInvokedListener"/>
        <listener class-name="Listeners.ITestListener"/>
